#include<iostream>
#include<math.h>
using namespace std;

double multiply(double num, int n){
    double ans =1.0;
    for(int i =1 ; i<=n ; i++){
        ans =ans * num;
    }
    return ans;
}

double getRoot(int n, int m){
    double low =1;
    double high =m;
    double esp =1e-6;//absolne

    while ((high-low)>esp)
    {
        double mid = (low+high)/2.0;
        if(multiply(mid , n)< m)
        low =mid;
        else 
        high = mid;

    }
    cout<<low<<" "<<high<<endl;

    cout<<pow(m,(double)(1.0/(double)n));
    
}

int main(){
    int m,n;
    cin>>n>>m;
    getRoot(n,m);
    return 0;
}
